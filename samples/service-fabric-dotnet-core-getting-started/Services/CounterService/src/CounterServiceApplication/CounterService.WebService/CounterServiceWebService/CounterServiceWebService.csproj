<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <Description>Stateless Web Service for Stateful CounterService App</Description>
    <Authors> </Authors>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <AssemblyName>CounterServiceWebService</AssemblyName>
    <OutputType>Exe</OutputType>
    <PackageId>CounterServiceWebService</PackageId>
  </PropertyGroup>
    <ItemGroup>
    <None Update="configuration.json;appsettings.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
